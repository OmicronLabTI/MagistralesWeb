{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "redis": {
    "hostname": "${REDIS_CONNECTION_STRING}"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.ColoredConsole", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "ColoredConsole",
        "Args": {
          "restrictedToMinimumLevel": "Information"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://${SEQLOG_SVC_SERVICE_HOST}:${SEQLOG_SVC_SERVICE_PORT_ALT}",
          "restrictedToMinimumLevel": "Information",
          "apiKey": "your-seq-api-key"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "ApplicationName": "Users"
    }
  },
  "SeqUrl": "http://${SEQLOG_SVC_SERVICE_HOST}:${SEQLOG_SVC_SERVICE_PORT_ALT}",
  "ConnectionStrings": {
    "DatabaseContext": "${POSTGRESS_NET_CONNECTION_STRING}"
  },
  "DiApiAddress": "${SAP_DI_API_URL}",
  "SwaggerAddress": "/api/pedidos",
  "AxityURL": "https://www.axity.com/",
  "SapAdapterURL": "http://sapadapter-svc.default.svc.cluster.local/",
  "ReportingService": "http://reporting-svc.default.svc.cluster.local/",
  "UserUrl": "http://usuarios-svc.default.svc.cluster.local/",
  "SapFileUrl": "${SAP_FILE_URL}",
  "OmicronFilesAddress": "${OMICRON_FILE_URL}",
  "QrImagesBaseRoute": "http://192.168.0.83:5103/Resources/",
  "AlmacenUrl": "http://almacen-svc.default.svc.cluster.local/",
  "kafka": {
    "finalizeproductionordersap": {
      "EH_FQDN": "omicronlabprodnsp2.servicebus.windows.net:9093",
      "EH_NAME": "finalizeproductionordersapprod",
      "EH_CONNECTION_STRING": "${EVENTHUB_NET_CONNECTION_FINALIZEPRODUCTIONORDERSAP}",
      "CONSUMER_GROUP": "finalizeproductionordersapprod",
      "CA_CERT_LOCATION": ""
    },
    "finalizeproductionorderpostgresql": {
      "EH_FQDN": "omicronlabprodnsp2.servicebus.windows.net:9093",
      "EH_NAME": "finalizeproductionorderpostgresqlprod",
      "EH_CONNECTION_STRING": "${EVENTHUB_NET_CONNECTION_FINALIZEPRODUCTIONORDERPOSTGRESQL}",
      "CONSUMER_GROUP": "finalizeproductionorderpostgresqlprod",
      "CA_CERT_LOCATION": ""
    },
    "productionorderpdfgeneration": {
      "EH_FQDN": "omicronlabprodnsp2.servicebus.windows.net:9093",
      "EH_NAME": "productionorderpdfgenerationprod",
      "EH_CONNECTION_STRING": "${EVENTHUB_NET_CONNECTION_PRODUCTIONORDERPDFGENERATION}",
      "CONSUMER_GROUP": "productionorderpdfgenerationprod",
      "CA_CERT_LOCATION": ""
    },
    "retryfinalizeproductionorder": {
      "EH_FQDN": "${KAFKA_EH_FQDN_2}",
      "EH_NAME": "${KAFKA_RETRYFINALIZEORDER_EH_NAME}",
      "EH_CONNECTION_STRING": "${KAFKA_RETRYFINALIZEORDER_EH_CONNECTION}",
      "CONSUMER_GROUP": "${KAFKA_RETRYFINALIZEORDER_CONSUMER_GROUP}"
    }
  },
  "ElasticApm": {
    "SecretToken": "${ELASTIC_APM_TOKEN}",
    "ServerUrls": "${ELASTIC_APM_URL}",
    "ServiceName": "Pedidos",
    "Environment": "Production"
  },
  "AzureAccountName": "${STORAGE_ACCOUNT}",
  "AzureAccountKey": "${STORAGE_ACCOUNT_KEY}",
  "OrderQrContainer": "orderqr",
  "DeliveryQrContainer": "deliveryqr",
  "InvoiceQrContainer": "invoiceqr",
  "SapServiceLayerAdapterUrl": "http://sapservicelayeradapter-svc.default.svc.cluster.local/"
}
