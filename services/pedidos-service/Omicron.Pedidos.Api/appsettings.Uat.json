{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "spring": {
    "application": {
      "name": "PedidosService"
    },
    "cloud": {
      "discovery": {
        "registrationMethod": "direct"
      }
    }
  },
  "redis": {
    "hostname": "${REDISDATA_SVC_SERVICE_HOST}:6379"
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://${APIMANAGER_SVC_SERVICE_HOST}:${APIMANAGER_SVC_SERVICE_PORT}/eureka/",
      "shouldFetchRegistry": true,
      "ValidateCertificates": false
    },
    "instance": {
      "port": 5103,
      "instanceId": "${spring:application:name}",
      "preferIpAddress": true,
      "leaseRenewalIntervalInSeconds": 5,
      "leaseExpirationDurationInSeconds": 5
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.ColoredConsole",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "ColoredConsole",
        "Args": {
          "restrictedToMinimumLevel": "Information"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://${SEQLOG_SVC_SERVICE_HOST}:${SEQLOG_SVC_SERVICE_PORT_ALT}",
          "restrictedToMinimumLevel": "Information",
          "apiKey": "your-seq-api-key"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "ApplicationName": "Users"
    }
  },
  "SeqUrl": "http://192.168.0.83:5341",
  "ConnectionStrings": {
    "DatabaseContext": "Server=192.168.0.83;Port=5432;Uid=omicronuser;Pwd=Axity2020;Database=omicrondev;Pooling=true;MinPoolSize=1;MaxPoolSize=500;Timeout=10"
  },
  "DiApiAddress": "http://172.30.5.49:5100/SapDiApi/",
  "SwaggerAddress": "/api/pedidos",
  "AxityURL": "https://www.axity.com/",
  "SapAdapterURL": "http://sapadapterservice/",
  "ReportingService": "http://reportingservice/",
  "UserUrl": "http://usuariosservice/",
  "SapFileUrl": "http://172.30.5.49:5103/SapFile/",
  "OmicronFilesAddress": "http://172.30.5.49:5002/",
  "QrImagesBaseRoute": "http://201.163.179.28:8000/api/pedidos/resources/",
  "AlmacenUrl": "http://almacenservice/",
  "kafka": {
    "EH_FQDN": "${KAFKA_SERVICE_SERVICE_HOST}:${KAFKA_SERVICE_SERVICE_PORT_KAFKA_PORT}",
    "EH_NAME": "insertlogs",
    "EH_CONNECTION_STRING": "",
    "CONSUMER_GROUP": "",
    "CA_CERT_LOCATION": "./cacert.pem"
  }
}