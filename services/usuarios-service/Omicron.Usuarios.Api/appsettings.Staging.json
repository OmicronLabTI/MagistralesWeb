{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "redis": {
    "hostname": "localhost:6379"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.ColoredConsole", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "ColoredConsole",
        "Args": {
          "restrictedToMinimumLevel": "Information"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://192.168.0.83:5341",
          "restrictedToMinimumLevel": "Information",
          "apiKey": "your-seq-api-key"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "ApplicationName": "Users"
    }
  },
  "SeqUrl": "http://192.168.0.83:5341",
  "ConnectionStrings": {
    "DatabaseContext": "Server=192.168.0.83;Port=5432;Uid=omicronuser;Pwd=Axity2020;Database=omicrondev;Pooling=true;MinPoolSize=1;MaxPoolSize=500;Timeout=5"
  },
  "ElasticApm": {
    "SecretToken": "",
    "ServerUrls": "",
    "ServiceName": "Usuarios",
    "Environment": "Staging"
  },
  "SwaggerAddress": "/api/usuarios",
  "AxityUrl": "https://www.axity.com/",
  "PedidosUrl": "http://pedidos-svc.default.svc.cluster.local",
  "SapAdapterUrl": "http://sapadapter-svc.default.svc.cluster.local"
}
