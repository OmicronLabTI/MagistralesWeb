{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "redis": {
    "hostname": "localhost:6379"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.ColoredConsole", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "ColoredConsole",
        "Args": {
          "restrictedToMinimumLevel": "Information"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://${SEQLOG_SVC_SERVICE_HOST}:${SEQLOG_SVC_SERVICE_PORT_ALT}",
          "restrictedToMinimumLevel": "Information",
          "apiKey": "your-seq-api-key"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "ApplicationName": "SapServiceLayerAdapter"
    }
  },
  "SeqUrl": "http://${SEQLOG_SVC_SERVICE_HOST}:${SEQLOG_SVC_SERVICE_PORT_ALT}",
  "HealthChecks-UI": {
    "HealthChecks": [
      {
        "Name": "Service Health Meter",
        "Uri": "http://localhost:5208/hc"
      }
    ],
    "EvaluationTimeOnSeconds": 10,
    "MinimumSecondsBetweenFailureNotifications": 60
  },
  "ElasticApm": {
    "SecretToken": "",
    "ServerUrls": "",
    "ServiceName": "SapServiceLayerAdapter",
    "Environment": "UAT"
  },
  "SAPServiceLayer": {
    "User": "${SAP_SERVICE_LAYER_USER}",
    "Pwd": "${SAP_SERVICE_LAYER_PWD}",
    "CompanyDb": "${SAP_SERVICE_LAYER_DB}"
  },
  "SAPServiceLayerService": "${SAP_SERVICE_LAYER_URL}",
  "CustomPropertiesName": {
    "CFDI": "U_CFDI_Provisional",
  }
}
