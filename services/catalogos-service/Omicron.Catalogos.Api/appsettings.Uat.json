{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "redis": {
    "hostname": "${REDIS_CONNECTION_STRING}"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.ColoredConsole", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "ColoredConsole",
        "Args": {
          "restrictedToMinimumLevel": "Information"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://${SEQLOG_SVC_SERVICE_HOST}:${SEQLOG_SVC_SERVICE_PORT_ALT}",
          "restrictedToMinimumLevel": "Information",
          "apiKey": "your-seq-api-key"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "ApplicationName": "Users"
    }
  },
  "SeqUrl": "http://${SEQLOG_SVC_SERVICE_HOST}:${SEQLOG_SVC_SERVICE_PORT_ALT}",
  "ConnectionStrings": {
    "DatabaseContext": "${POSTGRESS_NET_CONNECTION_STRING}"
  },
  "SwaggerAddress": "/api/catalogos",
  "ElasticApm": {
    "SecretToken": "",
    "ServerUrls": "",
    "ServiceName": "CatalogsMagistral",
    "Environment": "UAT"
  },
  "AzureAccountName": "omicronuat",
  "AzureAccountKey": "a09nXp0s3atDoeQ6GnHXwpx8KL0njK1SjtOy5GlGoG8P1jTGY4YoICh0seAoWf7BUKGFE40DFPeAnRtpIaE/Zw==",
  "SapAdapterURL": "http://sapadapter-svc.default.svc.cluster.local/",
  "CatalogsUrl": "http://catalogsdxp-svc.default.svc.cluster.local/",
  "WarehousesFileUrl": "https://omicronuat.blob.core.windows.net/resources/Warehouses.xlsx",
  "ManufacturersFileUrl": "https://omicronuat.blob.core.windows.net/resources/Manufacturers.xlsx",
  "ProductTypeColorsFileUrl": "https://omicronuat.blob.core.windows.net/resources/ProductTypeColors.xlsx"
}
