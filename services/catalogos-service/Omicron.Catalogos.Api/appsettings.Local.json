{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "redis": {
    "hostname": "localhost:6379"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.ColoredConsole",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "ColoredConsole",
        "Args": {
          "restrictedToMinimumLevel": "Information"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "restrictedToMinimumLevel": "Information",
          "apiKey": "your-seq-api-key"
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ],
    "Properties": {
      "ApplicationName": "Users"
    }
  },
  "SeqUrl": "http://localhost:5341",
  "ConnectionStrings": {
    "DatabaseContext": "${POSTGRESS_NET_CONNECTION_STRING}"
  },
  "SwaggerAddress": "",
  "ElasticApm": {
    "SecretToken": "",
    "ServerUrls": "",
    "ServiceName": "CatalogsMagistral",
    "Environment": "Local"
  },
  "AzureAccountName": "${STORAGE_ACCOUNT}",
  "AzureAccountKey": "${STORAGE_ACCOUNT_KEY}",
  "SapAdapterURL": "http://localhost:5102/",
  "CatalogsUrl": "http://localhost:5202/",
  "WarehousesFileUrl": "https://omicronsaprod.blob.core.windows.net/resources/Warehouses.xlsx",
  "ManufacturersFileUrl": "https://omicronsaprod.blob.core.windows.net/resources/Manufacturers.xlsx",
  "ProductTypeColorsFileUrl": "https://omicronsaprod.blob.core.windows.net/resources/ProductTypeColors.xlsx"
}